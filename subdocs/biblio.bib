  Title                    = {Competencies for successful self-regulated learning in higher education: structural model and indications drawn from expert interviews},
  Author                   = {Dresel, Markus and Schmitz, Bernhard and Schober, Barbara and Spiel, Christine and Ziegler, Albert and Engelschalk, Tobias and J{\"{o}}stl, Gregor and Klug, Julia and Roth, Anne and Wimmer, Bastian and Steuer, Gabriele},
  Journal                  = {Studies in Higher Education},
  Year                     = {2015},
  Number                   = {3},
