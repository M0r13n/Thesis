@Article{yastibas_use_2015,
  Title                    = {The {Use} of {E}-portfolio-based {Assessment} to {Develop Students}{\textquoteright} {Self}-regulated {Learning} in {English Language Teaching}},
  Author                   = {Yastibas, Ahmet Erdost and Yastibas, G{\"{u}}lsah Cinar},
  Journal                  = {Procedia - Social and Behavioral Sciences},
  Year                     = {2015},
  Pages                    = {3--13},
  Volume                   = {176},

  Abstract                 = {Traditional assessment ways like multiple choice tests have been used to evaluate students{\textquoteright} performance in English language teaching (ELT) at schools, but these assessment methods are not effective and efficient because they do not show the real performance of students. On the other hand, student-centeredness is the main focus of many teaching and learning methods and techniques used in ELT. It emphasizes learning by doing. As the main focus is on learning by doing, assessing this process requires different assessment methods that take factors, such as students{\textquoteright} understanding and personal difference into consideration, while evaluating learners{\textquoteright} performance. Therefore, educators need to develop new student-centred assessment methods to evaluate learners in ELT. One of these ways is the use of e-portfolios. E-portfolios can be used as an assessment in courses effectively to improve different learning styles of learners. One of them is self-regulated learning, which focuses on students{\textquoteright} taking responsibility for their learning from the beginning to the end. This process includes setting goals and organizing learning environments according to their determined goals, but finding research on the use of e-portfolios as an assessment tool in self-regulated learning in ELT is rare. Therefore, this paper aims to find out whether e-portfolio-based assessment can be used to improve students{\textquoteright} self-regulated learning through reviewing the literature. The result of the literature review indicates that e- portfolio-based assessment can develop students{\textquoteright} self-regulated learning in ELT.},
  Doi                      = {10.1016/j.sbspro.2015.01.437},
  File                     = {ScienceDirect Snapshot:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/X9IS3NQJ/S1877042815004747.html:URL},
  ISSN                     = {1877-0428},
  Keywords                 = {E-portfolio, E-portfolio based assessment, Self-regulated learning},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S1877042815004747},
  Urldate                  = {2016-09-13}
}

