  Shorttitle               = {Analyzing {Longitudinal} {Data} with {Multilevel} {Models}},
  Url                      = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2613314/},
  Urldate                  = {2015-02-22}
