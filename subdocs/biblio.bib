  ISSN                     = {0021-843X},
  Keywords                 = {Emotional Regulation, emotion regulation, longitudinal, Longitudinal Studies, nonsuicidal self-injury, rumination, Rumination (Cognitive Process), Self Injurious Behavior},
  Shorttitle               = {Change in emotion regulation strategy use and its impact on adolescent nonsuicidal self-injury},
  Url                      = {http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=2014-24818-001&site=ehost-live},
