@Article{berthold_learning_2007,
  Title                    = {Do learning protocols support learning strategies and outcomes? {The} role of cognitive and metacognitive prompts},
  Author                   = {Berthold, Kirsten and N{\"{u}}ckles, Matthias and Renkl, Alexander},
  Journal                  = {Learning and Instruction},
  Year                     = {2007},
  Number                   = {5},
  Pages                    = {564--577},
  Volume                   = {17},

  Abstract                 = {Although writing learning protocols is an effective follow-up course work activity, many learners tend to do it in a rather suboptimal way. Hence, we analyzed the effects of instructional support in the form of prompts. The effects of different types of prompts were investigated in an experiment with four conditions: cognitive prompts, metacognitive prompts, a combination of cognitive and metacognitive prompts, or no prompts (N = 84 undergraduate psychology students). We found that the prompts stimulated the elicitation of cognitive and metacognitive learning strategies. The provision of purely metacognitive prompts did not, however, improve learning outcomes. Only the groups who had received cognitive, or a combination of cognitive and metacognitive, prompts learned more than the control group. This effect was mediated by cognitive learning strategies. The learners in the successful groups did not perceive the prompted learning strategies as more helpful than the learners of the group without prompts. It can be concluded that cognitive prompts---alone or in combination with metacognitive prompts---are an effective means to foster learning. However, additional means should be employed in order to convince the learner of the usefulness of such prompts.},
  Doi                      = {10.1016/j.learninstruc.2007.09.007},
  File                     = {Berthold_et_al_2007_Do_learning_protocols_support_learning_strategies.pdf.pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/6H2QC8QK/Berthold_et_al_2007_Do_learning_protocols_support_learning_strategies.pdf.pdf:PDF;ScienceDirect Snapshot:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/D9JJJ47X/S0959475207000990.html:URL},
  ISSN                     = {0959-4752},
  Keywords                 = {Cognitive learning strategies, Cognitive learning strategies, Journal writing, Journal writing, Learning protocol, Learning protocol, Learning strategies, Learning strategies, Metacognitive learning strategies, Metacognitive learning strategies, Prompts, Prompts},
  Shorttitle               = {Do learning protocols support learning strategies and outcomes?},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0959475207000990},
  Urldate                  = {2014-09-03}
}

