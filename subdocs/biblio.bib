@Article{ziegler_research_2010,
  Title                    = {Research on a modified framework of implicit personality theories},
  Author                   = {Ziegler, Albert and Stoeger, Heidrun},
  Journal                  = {Learning and Individual Differences},
  Year                     = {2010},
  Number                   = {4},
  Pages                    = {318--326},
  Volume                   = {20},

  Abstract                 = {There is ample evidence that labeled gifted students exhibit maladaptive behavior patterns. According to Carol Dweck those students who subscribe to a fixed view of their abilities are particularly at risk. In this contribution we extended Dweck's framework and distinguished two aspects of the implicit theory of one's own abilities. We hypothesized that the negative consequences of a fixed view are limited to the belief that one's own deficits are stable. In contrast, we assumed that the belief in the stability of existing abilities as well as the belief in the modifiability of ability deficits is adaptive. In two longitudinal studies with students from grades 7 to 10 we found supportive evidence for the proposed distinction.},
  Doi                      = {10.1016/j.lindif.2010.01.007},
  File                     = {ScienceDirect Snapshot:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/6W8V72QS/S1041608010000087.html:URL;Ziegler_Stoeger_2010_Research_on_a_modified_framework_of_implicit.pdf.pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/MC4V595V/Ziegler_Stoeger_2010_Research_on_a_modified_framework_of_implicit.pdf.pdf:PDF},
  ISSN                     = {1041-6080},
  Keywords                 = {Gifted, Gifted, Implicit personality theories, Implicit personality theories, Intelligence, Intelligence},
  Series                   = {New {Dimensions}: {Gifted} {Education} in the 21st {Century}},
