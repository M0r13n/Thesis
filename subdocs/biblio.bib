@Article{kotsopoulos_electronic_2015,
  Title                    = {Electronic portfolios in grades one, two and three: {A} cautionary tale},
  Author                   = {Kotsopoulos, Donna and Lee, Joanne and Cordy, Michelle and Bruyns, Susan},
  Journal                  = {Technology, Pedagogy and Education},
  Year                     = {2015},
  Number                   = {5},
  Pages                    = {1--16},
  Volume                   = {24},

  Abstract                 = {Some electronic portfolios (EPs) developers are proposing that EPs are suitable for implementation in primary education (i.e. kindergarten to grade three). Yet, empirical research evaluating the implementation and efficacy of EPs used in primary school settings at both the teacher and the student level is scarce. In this research, the authors attempted to implement EPs in grades one, two and three (approximate ages 6 through 8). EPs were reviewed to make the EP selection and in-depth collaborative planning with the teachers ensued. The main challenges for teachers and students were at the user interface level. Results from this cautionary tale suggest that implementation of EPs in primary education may be premature. While much potential exists in EPs, it has yet to be realised. Recommendations for improving EPs are suggested. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  Doi                      = {10.1080/1475939X.2014.961952},
  ISSN                     = {1475-939X},
  Keywords                 = {2015, Collaboration, Electronic Learning, electronic portfolios, Elementary education, learning, primary education, teaching},
  Shorttitle               = {Electronic portfolios in grades one, two and three}
}

