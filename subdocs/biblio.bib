@Article{bjork_self-regulated_2013,
  Title                    = {Self-regulated learning: beliefs, techniques, and illusions.},
  Author                   = {Bjork, Robert A and Dunlosky, John and Kornell, Nate},
  Journal                  = {Psychology},
  Year                     = {2013},
  Pages                    = {417--444},
  Volume                   = {64},

  Abstract                 = {Knowing how to manage one's own learning has become increasingly important in recent years, as both the need and the opportunities for individuals to learn on their own outside of formal classroom settings have grown. During that same period, however, research on learning, memory, and metacognitive processes has provided evidence that people often have a faulty mental model of how they learn and remember, making them prone to both misassessing and mismanaging their own learning. After a discussion of what learners need to understand in order to become effective stewards of their own learning, we first review research on what people believe about how they learn and then review research on how people's ongoing assessments of their own learning are influenced by current performance and the subjective sense of fluency. We conclude with a discussion of societal assumptions and attitudes that can be counterproductive in terms of individuals becoming maximally effective learners.},
  Doi                      = {10.1146/annurev-psych-113011-143823},
  File                     = {Bjork_et_al_2013_Self-regulated_learning.pdf.pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/UMVIJZCT/Bjork_et_al_2013_Self-regulated_learning.pdf.pdf:PDF}
}

@Article{blau_interacting_2013,
  Title                    = {Interacting for learning: {Digital} portfolios for a learning community in a university course},
  Author                   = {Blau, Ina and Mor, Nili and Neuthal, Tami},
  Journal                  = {Learning, Media and Technology},
  Year                     = {2013},
  Number                   = {3},
  Pages                    = {241--255},
  Volume                   = {38},

  Abstract                 = {This study investigates student interactions in a blog-based learning community in a university course. In addition, this study explores the dynamics of group interactions in individual blog-based environments compared with collaborative wiki-based educational activities. A learning community of 56 graduate students wrote individual blogs and weekly group summaries using a wiki environment. The posts were analyzed by identifying a post content type and explicit feedback promotions, and by counting the number of the blogger's own comments and the number of others{\textquoteright} comments per post, received from peers studying in the same versus the other offline group. The results show that choosing the appropriate type of post content (i.e., sharing experiences rather than providing information, explicitly calling for feedback, and providing the blogger's own comments) augments peer interaction in a blog environment, thus explaining 51.4\% of variance in peer comments. Group interactions through comments in individual blogs, without the possibility of changing one another's content, seem to be non-intrusive and prompt interactions with offline as well as with online peers. In contrast, actual editing of texts through multi-authored collaborative wikis remained affected by student offline former acquaintance. The results are discussed in terms of social interactions and pedagogical beliefs. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  Doi                      = {10.1080/17439884.2012.709864},
  ISSN                     = {1743-9884},
  Keywords                 = {2013, College Students, digital portfolios, Digital Video, Education, educational activities, group dynamics, group interactions, learning, learning community, Social Interaction, social interactions, students},
  Shorttitle               = {Interacting for learning}
}

@Article{boekaerts_self-regulated_1998,
  Title                    = {Self-regulated learning: where we are today},
  Author                   = {Boekaerts, Monique},
  Journal                  = {International Journal of Educational Research},
  Year                     = {1998},
  Number                   = {6},
  Pages                    = {445--457},
  Volume                   = {31},

  Abstract                 = {Self-regulated learning has emerged as an important new construct in education. Our understanding of self-regulated learning has been informed by three schools of thought: (1) research on learning styles, (2) research on metacognition and regulation styles, and (3) theories of the self, including goal-directed behavior. Based on these schools of thought, a three-layer model is presented. The innermost layer pertains to regulation of the processing modes. The middle layer represents regulation of the learning process. The outermost layer concerns regulation of the self. Educators and researchers would benefit from an integration of these three frames of reference into a comprehensive model of self-regulated learning.},
  Doi                      = {10.1016/S0883-0355(99)00014-2},
  File                     = {Attachment:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/4S2DWNZ8/92701D9A-3D94-4FE5-B204-798F665EF57C:PDF;Boekaerts_1998_Self-regulated_learning.pdf.pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/II7MCJWZ/Boekaerts_1998_Self-regulated_learning.pdf.pdf:PDF},
  Url                      = {http://linkinghub.elsevier.com/retrieve/pii/S0883035599000142}
}

@Article{boekaerts_self-regulation_2005,
  Title                    = {Self-{Regulation} in the {Classroom}: {A Perspective} on {Assessment} and {Intervention}},
  Author                   = {Boekaerts, Monique and Corno, Lyn},
  Journal                  = {Applied Psychology},
  Year                     = {2005},
  Number                   = {2},
  Pages                    = {199--231},
  Volume                   = {54},

  Abstract                 = {Il n{\textquoteright}y a pas de d{\'{e}}finition simple et univoque du concept d{\textquoteright}apprentissage autor{\'{e}}gul{\'{e}}. Des th{\'{e}}oriciens de psychologie de l{\textquoteright}{\'{e}}ducation ont r{\'{e}}duit l{\textquoteright}{\'{e}}ventail des aptitudes des {\'{e}}l{\`{e}}ves {\`{a}} s{\textquoteright}autor{\'{e}}guler en se focalisant sur le versant scolaire de l{\textquoteright}{\'{e}}ducation, {\`{a}} savoir l{\textquoteright}acquisition des connaissances et les objectifs de r{\'{e}}ussite. Toutefois, le monde complexe de l{\textquoteright}{\'{e}}tude en classe engendre une situation o{\`{u}} diff{\'{e}}rents buts entrent en concurrence aux yeux des {\'{e}}l{\`{e}}ves. Le mod{\`{e}}le d{\textquoteright}autor{\'{e}}gulation {\`{a}} double processus de Bookaerts montre que les deux objectifs que sont l{\textquoteright}{\'{e}}tude et le bien-{\^{e}}tre interagissent. Nous estimons que lorsque les {\'{e}}l{\`{e}}ves ont acc{\`{e}}s {\`{a}} des strat{\'{e}}gies autonomes bien au point se traduisant par de bonnes habitudes de travail, ils ont une plus grande probabilit{\'{e}} de se motiver pour les {\'{e}}tudes et de sauvegarder leur bien-{\^{e}}tre quand une source de stress bloque l{\textquoteright}apprentissage. There is no simple and straightforward definition of the construct of self-regulated learning. Theorists in educational psychology have narrowed the scope of students{\textquoteright} capability to self-regulate through a focus on the academic side of education, namely on learning and achievement goals. However, the messy world of classroom learning creates a situation in which different goals compete for students{\textquoteright} attention. Boekaerts{\textquoteright} dual processing self-regulation model describes how learning goals interact with well-being goals. We propose that when students have access to well-refined volitional strategies manifested as good work habits, they are more likely to invest effort in learning and get off the well-being track when a stressor blocks learning. Shifting definitions of SRL have led to changing measurement procedures; researchers moved away from decontextualised measures of SRL to domain-specific measures and then on to context-sensitive measures. The validity and reliability of the first generation of SR assessment has been limited and several issues remain. Recently, researchers have designed assessment packages including new instruments that better capture self-regulation as a process (including for example traces of mental events, situational manipulations, and records of student work strategies). A combination of instruments is preferable over a single instrument for assessing self-regulation as a process and the effects of interventions to improve students{\textquoteright} self-regulatory capacity. At present, many sound SRL interventions exist and some general lessons can be learned about classroom intervention research.},
  Doi                      = {10.1111/j.1464-0597.2005.00205.x},
  File                     = {Boekaerts und Corno - 2005 - Self-Regulation in the Classroom A Perspective on.pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/B3QSSD5A/Boekaerts und Corno - 2005 - Self-Regulation in the Classroom A Perspective on.pdf:PDF;Snapshot:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/J6WVX3DI/full.html:URL},
  ISSN                     = {1464-0597},
  Language                 = {en},
  Shorttitle               = {Self-{Regulation} in the {Classroom}},
  Urldate                  = {2016-05-23}
}

