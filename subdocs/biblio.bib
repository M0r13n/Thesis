@Book{sellin_varianten_2003,
  Title                    = {Varianten der {Selbstwertschaetzung} und {Hilfesuche}},
  Author                   = {Sellin, Ina},
  Year                     = {2003},
  Series                   = {Technische {Universitaet} {Chemnitz}, {Philosophische} {Fakultaet}},

  Abstract                 = {Die Beziehung zwischen Varianten der Selbstwertschaetzung und Hilfesuche wird untersucht. In einer Fragebogenstudie und in einem Quasiexperiment wurden Daten an Stichproben von 178 Psychologie-Studierenden erhoben. Es zeigte sich, dass das Ausmass der Hilfesuche allein durch die Verwendung der Hoehe der Selbstwertschaetzung nicht vorhersagt werden kann. Wurden jedoch weitere Variablen zur Beschreibung der Selbstwertschaetzung herangezogen, konnten Aussagen zur Beziehung zwischen Selbstwertschaetzung und Hilfesuche abgeleitet werden. Es wurden Teilgruppen von Personen mit Hilfe von Clusteranalysen gebildet, die sich durch ihre Auspraegungen auf den Variablen Selbstwertniveau, Selbstwertinstabilitaet und Narzissmus charakterisieren liessen. Diese Varianten der Selbstwertschaetzung unterschieden sich in ihrer Hilfesuche. So waehlten Personen mit einer hohen und zeitlich stabilen Selbstwertschaetzung insbesondere Hilfeangebote, die sie befaehigten, aehnliche Probleme in Zukunft selbststaendig und autonom zu bewaeltigen. Personen mit einer Selbstwertschaetzung mit narzisstischen Tendenzen vermieden es haeufig, Hilfen in Anspruch zu nehmen, auch wenn das Nutzen der angebotenen Hilfen in der Situation angemessen gewesen waere. Eine weitere Teilgruppe bildeten Personen mit hoher aber zeitlich instabiler Selbstwertschaetzung. Diese nutzten meist mehr angebotene Hilfen als die anderen Gruppen. Es wird geschlussfolgert, dass sich Personen mit hoher und zeitlich stabiler Selbstwertschaetzung bei der Hilfesuche meist angemessen und adaptiv verhalten, waehrend Personen anderer Varianten der Selbstwertschaetzung zum Teil zu viel oder zum Teil zu wenig Hilfe in Anspruch nehmen. (Autor/R.N. - ZPID)},
  File                     = {Sellin_2003_Varianten_der_Selbstwertschaetzung_und_Hilfesuche.pdf.pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/CI7F4EJJ/Sellin_2003_Varianten_der_Selbstwertschaetzung_und_Hilfesuche.pdf.pdf:PDF},
  ISBN                     = {723028188},
  Url                      = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=psyn&AN=0169040}
}

@Article{seman_e-teaching_2012,
  Title                    = {E-teaching {Portfolio Implementation} using {Mahara Open Source Management System}},
  Author                   = {Seman, Sharidatul Akma Abu and Wan Rashid, Wan Edura and Nasir, Hazira Mohd},
  Journal                  = {Procedia - Social and Behavioral Sciences},
  Year                     = {2012},
  Pages                    = {974--979},
  Volume                   = {65},

  Abstract                 = {Teaching portfolio has been widely used in higher education as a useful management tool for monitoring and improving teaching and learning process among academicians. The usage of portfolio methods to document professional development in teaching is well known by all. However, despite its proliferation, this issue is still dogged among academicians. Time pressure, data are not integrated and not easily accessible as well as paper-based portfolio seems to be the critical factors why teaching portfolio may become a burden for any academicians. E- teaching portfolio allows all academicians to be involved in the community over network technologies with the capability to store various types of media. E-Teaching Portfolio helps in preparing evidence in supporting teaching and learning processes, as a reflective tool in enhancing their capabilities and as a way in supporting their self- assessment review. Therefore, the aim of this paper is to create a basic understanding of e-teaching portfolio in higher education prior to replace the paper-based portfolio. An action research methodology was adapted consisting of four important phases diagnosing, planning action, taking action and evaluating action. A sample prototype was created using an open-source management tool, Mahara. Mahara is a fully featured open source electronic portfolio, which was developed in 2006 by the New Zealand e-Portfolio Project funded by the NZ Tertiary Education Commission. Mahara platform was chosen in compiling and integrating all files and academician's portfolios. Furthermore, this paper will deliberate discuss the implementation of an e-teaching portfolio using the Mahara platform to support teaching and learning.},
  Doi                      = {10.1016/j.sbspro.2012.11.229},
  File                     = {E-teaching Portfolio Implementation using Mahara Open Source Management System - 1-s2.0-S1877042812052147-main.pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/RMG7VV48/1-s2.0-S1877042812052147-main.pdf:PDF;ScienceDirect Snapshot:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/QBWM4TZI/S1877042812052147.html:URL},
  ISSN                     = {1877-0428},
  Keywords                 = {Academicians, E-teaching portfolio, Higher Education, Mahara platform, Open souce management system},
  Series                   = {International {Congress} on {Interdisciplinary} {Business} and {Social} {Sciences} 2012 ({ICIBSoS} 2012)},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S1877042812052147},
  Urldate                  = {2016-09-13}
}

