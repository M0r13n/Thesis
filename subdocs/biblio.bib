@Article{gino_evil_2014,
  Title                    = {Evil {Genius}? {How Dishonesty Can Lead} to {Greater Creativity}},
  Author                   = {Gino, Francesca and Wiltermuth, Scott S.},
  Journal                  = {Psychological Science},
  Year                     = {2014},
  Number                   = {4},
  Pages                    = {973--981},
  Volume                   = {25},

  Abstract                 = {We propose that dishonest and creative behavior have something in common: They both involve breaking rules. Because of this shared feature, creativity may lead to dishonesty (as shown in prior work), and dishonesty may lead to creativity (the hypothesis we tested in this research). In five experiments, participants had the opportunity to behave dishonestly by overreporting their performance on various tasks. They then completed one or more tasks designed to measure creativity. Those who cheated were subsequently more creative than noncheaters, even when we accounted for individual differences in their creative ability (Experiment 1). Using random assignment, we confirmed that acting dishonestly leads to greater creativity in subsequent tasks (Experiments 2 and 3). The link between dishonesty and creativity is explained by a heightened feeling of being unconstrained by rules, as indicated by both mediation (Experiment 4) and moderation (Experiment 5).},
  Doi                      = {10.1177/0956797614520714},
  File                     = {Gino_Wiltermuth_2014_Evil_Genius.pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/KN9QR35F/Gino_Wiltermuth_2014_Evil_Genius.pdf:PDF;Snapshot:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/FA94EPXH/973.html:URL},
  ISSN                     = {0956-7976, 1467-9280},
  Keywords                 = {creativity, decision making, dishonesty, ethics, moral flexibility, morality, rule breaking},
  Language                 = {en},
  Pmid                     = {24549296},
  Shorttitle               = {Evil {Genius}?},
  Url                      = {http://pss.sagepub.com/content/25/4/973},
  Urldate                  = {2015-01-17}
}

@Article{glaser-zikuda_lehramtsstudierende_2007,
  Title                    = {Lehramtsstudierende reflektieren und evaluieren ihr {Unterrichtshandeln} --- zum {Potenzial} des {Tagebuch-Ansatzes}},
  Author                   = {Gl{\"{a}}ser-Zikuda, Michaela},
  Journal                  = {Gruppendynamik und Organisationsberatung},
  Year                     = {2007},
  Number                   = {1},
  Pages                    = {43--57},
  Volume                   = {38},

  Abstract                 = {Die Verbesserung in der Lehrerbildung wird in vielerlei Hinsicht zunehmend bedeutsamer. Vor allem der Entwicklung zentraler Lehrkompetenzen in verschiedenen Phasen des Professionalisierungsprozesses kommt eine entscheidende Funktion zu. Im Rahmen der schulpraktischen Ausbildung kann es bereits in einem fr{\"{u}}hen Stadium gelingen, solche Kompetenzen bei den angehenden Lehrer innen zu entwickeln und zu f{\"{o}}rdern. In diesem Beitrag wird ein Konzept vorgestellt, das auf der Grundlage des Tagebuch-Ansatzes die Selbstreflexion und Selbstevaluation der Studierenden mit diversen Formen des Mentoring kombiniert, und so zu einer Verbesserung der Lehrerbildung beitragen kann.},
