@Article{soric_role_2009,
  Title                    = {The role of students{\textquoteright} interests in self-regulated learning: {The} relationship between students{\textquoteright} interests, learning strategies and causal attributions},
  Author                   = {Sori{\'{c}}, Izabela and Palek{\v{c}}i{\'{c}}, Marko},
