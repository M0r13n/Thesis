@Article{klein_modeling_2006,
  Title                    = {Modeling {Heterogeneity} of {Latent Growth Depending} on {Initial Status}},
  Author                   = {Klein, Andreas G. and Muth{\'{e}}n, Bengt O.},
  Journal                  = {Journal of Educational and Behavioral Statistics},
  Year                     = {2006},
  Number                   = {4},
  Pages                    = {357--375},
  Volume                   = {31},

  Abstract                 = {In this article, a heterogeneous latent growth curve model for modeling heterogeneity of growth rates is proposed. The suggested model is an extension of a conventional growth curve model and a complementary tool to mixed growth modeling. It allows the modeling of heterogeneity of growth rates as a continuous function of latent initial status and time-invariant covariates. A quasi-ML parameter estimation method and a likelihood-ratio test for heterogeneity of growth is developed, and the new methodology is applied to an empirical example on math achievement data. The example illustrates that the proposed model gives more accurate confidence intervals for the prediction of future math achievement than a conventional growth curve model and identifies the subgroup of subjects which show the most homogeneous growth.},
  Doi                      = {10.3102/10769986031004357},
  File                     = {Klein und Muthén - 2006 - Modeling Heterogeneity of Latent Growth Depending .pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/3RQ7MPG9/Klein und Muthén - 2006 - Modeling Heterogeneity of Latent Growth Depending .pdf:PDF;Klein und Muthén - 2006 - Modeling Heterogeneity of Latent Growth Depending .pdf:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/FQ69W9CB/Klein und Muthén - 2006 - Modeling Heterogeneity of Latent Growth Depending .pdf:PDF;Snapshot:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/92VKKVNM/357.html:URL;Snapshot:Users/bastianwimmer/Library/Application Support/Zotero/Profiles/elpp7n8h.default/zotero/storage/FERDBPU8/357.html:URL},
